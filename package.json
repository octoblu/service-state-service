{
  "name": "service-state-service",
  "version": "1.1.0",
  "description": "service-state-service an Octoblu Service",
  "keywords": [
    "Octoblu",
    "Service"
  ],
  "homepage": "https://github.com/octoblu/service-state-service",
  "bugs": "https://github.com/octoblu/service-state-service/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/octoblu"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/service-state-service.git"
  },
  "scripts": {
    "start": "node command.js",
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "request": "^2.74.0",
    "shmock": "^0.8.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "basicauth-middleware": "^2.0.0",
    "coffee-script": "^1.10.0",
    "debug": "^2.2.0",
    "express-octoblu": "^1.1.1",
    "lodash": "^4.15.0",
    "node-etcd": "^5.0.3",
    "server-destroy": "^1.0.1",
    "sigterm-handler": "^1.0.2"
  },
  "license": "MIT",
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
